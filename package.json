{
  "name": "my_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cargo make build-development-rust && npm-run-all -p _start:rust _start:webpack _start:mediaserver",
    "start:nowarnings": "cargo make build-development-rust-nowarnings && npm-run-all -p _start:rust-nowarnings _start:webpack _start:mediaserver",
    "deploy:all": "npm-run-all _manifest-generator deploy:nomanifest",
    "deploy:nomanifest": "npm-run-all _build-and-deploy _media-sync",
    "cors:set": "node ./build-utils/cors.js --app --set",
    "cors:get": "node ./build-utils/cors.js --app --get",
    "_manifest-generator": "cargo make manifest-generator",
    "_build-and-deploy": "cargo make build-and-deploy",
    "_media-sync": "node ./build-utils/media-server-sync.js --hard",
    "_start:rust": "makers watch-rust",
    "_start:rust-nowarnings": "makers watch-rust-nowarnings",
    "_start:webpack": "cross-env release_target=local webpack-dev-server --config webpack.dev.js",
    "_start:mediaserver": "node build-utils/local-media-server.js --hard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dakom/my_app.git"
  },
  "author": "David Komer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dakom/my_app/issues"
  },
  "homepage": "https://github.com/dakom/my_app#readme",
  "devDependencies": {
    "@types/dat.gui": "^0.7.5",
    "@types/jest": "^24.0.23",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "extract-loader": "^3.1.0",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "lit-css-loader": "0.0.3",
    "npm-run-all": "^4.1.5",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "shelljs": "^0.8.3",
    "text-encoding": "^0.7.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "lit-element": "^2.2.1"
  }
}
